# Smart Alpaca V2.0 Environment Variables Template
# Copy this file to .env and fill in your actual values
# Phase A: Paper Trading Configuration

# Environment
NODE_ENV=development
PORT=5000

# Database
DATABASE_URL=postgresql://your_username:your_password@localhost:5432/smart_alpaca

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0

# Alpaca API (Paper Trading - Safe for Development)
ALPACA_API_KEY=your_alpaca_api_key_here
ALPACA_SECRET_KEY=your_alpaca_secret_key_here
ALPACA_BASE_URL=https://paper-api.alpaca.markets
ALPACA_PAPER=true

# Google/Gemini AI
GEMINI_API_KEY=your_gemini_api_key_here

# Session Security
SESSION_SECRET=your_random_session_secret_minimum_32_characters_long

# Logging
LOG_LEVEL=info
